{
  "name": "snowpack-start",
  "version": "1.0.0",
  "description": "Start a new custom Snowpack application.",
  "keywords": [
    "snowpack",
    "custom",
    "start",
    "init",
    "create"
  ],
  "author": "awu43",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/awu43/snowpack-start.git"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "/dist",
    "/dist-files",
    "/dist-templates"
  ],
  "bin": {
    "snowpack-start": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "node postbuild.js",
    "generate": "ts-node --files test/generate.js",
    "gen-test": "ts-mocha \"test/src-templates-test.js\" && ts-mocha \"test/package-versions-test.js\"",
    "test": "ts-mocha \"test/*.test.js\"",
    "coverage": "nyc --reporter=html --reporter=text mocha \"test/*.test.js\"",
    "lint": "eslint \"src/**/*.{js,ts}\""
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/fs-extra": "^9.0.11",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@types/prompts": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "chai-files": "^1.4.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "json5": "^2.2.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.6.0",
    "tmp": "^0.2.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "execa": "^5.0.0",
    "fs-extra": "^9.1.0",
    "prompts": "^2.4.1"
  }
}
